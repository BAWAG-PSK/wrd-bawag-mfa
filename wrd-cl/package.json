    "build:mock": "ng run wrd-cl:build-mock",
    "build:prod": "ng run wrd-cl:build-prod",
    "build:prod:mock": "ng run wrd-cl:build-prod-mock",
    "server:start": "node src/server/server.js",
    "serve:mock": "ng serve --configuration=mock",
