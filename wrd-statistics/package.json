{
  "name": "wrd-statistics",
  "version": "1.0.0",
  "scripts": {
    "instrument:main-js": "./node_modules/.bin/gulp instrument:main.js",
    "generate:bootstrap-json": "./node_modules/.bin/gulp generate:bootstrap.json",
    "build": "ng build",
    "build:prod": "./node_modules/.bin/ng run wrd-statistics:build-ext-prod && npm run generate:bootstrap-json",
    "build:prod:mock": "./node_modules/.bin/ng run wrd-statistics:build-ext-mock && npm run instrument:main-js && npm run generate:bootstrap-json",
    "serve": "ng serve",
    "serve:mock": "./node_modules/.bin/ng serve --configuration=mock",
    "serve:ext:prod": "npm run build:prod && npm run server:start",
    "serve:ext:mock": "ng run wrd-statistics:build-ext-mock && npm run instrument:main-js && npm run generate:bootstrap-json && npm run server:start",
    "serve:standalone": "./node_modules/.bin/ng run wrd-statistics:build-standalone && npm run server:start",
    "server:start": "node src/server/server.js",
    "deploy": "./deploy.sh",
    "kill": "kill -9 $(lsof -t -i:50002)"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.13",
    "@angular/common": "~8.2.13",
    "@angular/compiler": "~8.2.13",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~8.2.13",
    "@angular/platform-browser": "~8.2.13",
    "@angular/platform-browser-dynamic": "~8.2.13",
    "@angular/router": "~8.2.13",
    "@clr/angular": "~2.2.0",
    "@clr/icons": "~2.3.1",
    "@clr/ui": "~2.2.0",
    "@swimlane/ngx-charts": "~12.1.0",
    "@webcomponents/custom-elements": "~1.0.0",
    "@wrd-web/shared": "file:../wrd-nexus/@wrd-web/shared/wrd-web-shared-0.0.2.tgz",
    "rxjs": "~6.4.0",
    "tslib": "~1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "~8.2.0",
    "@angular-builders/dev-server": "~7.3.1",
    "@angular-devkit/build-angular": "~0.803.16",
    "@angular/cli": "~8.3.16",
    "@angular/compiler-cli": "~8.2.13",
    "@angular/language-service": "~8.2.13",
    "@angularclass/hmr": "~2.1.3",
    "@angular-builders/jest": "~8.2.0",
    "@types/node": "~8.9.4",
    "@types/gulp": "~4.0.6",
    "@types/jest": "~24.0.18",
    "gulp": "~4.0.2",
    "share-loader": "0.7.0",
    "jsonfile": "~5.0.0",
    "last-call-webpack-plugin": "~3.0.0",
    "codelyzer": "~5.0.0",
    "jest": "~24.9.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
